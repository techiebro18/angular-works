/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@services/*": [
        "src/app/core/services/*"
      ],
      "@schemas/*": [
        "src/app/shared/schemas/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@environments/*": [
        "src/environments/*"
      ],

    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}

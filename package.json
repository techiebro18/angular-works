{
  "name": "private-sellers-spa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200 --disable-host-check",
    "dev": "ng serve ---configuration=local",
    "dev:ssr": "ng run private-sellers-spa:serve-ssr",
    "serve:ssr": "node dist/private-sellers-spa/server/main.js",
    "prerender": "ng run private-sellers-spa:prerender",
    "build": "ng build",
    "build:docker": "ng build --configuration=docker && ng run private-sellers-spa:server:development",
    "build:dev": "ng build --configuration=development",
    "build:dev-ssr": "ng build --configuration=development && ng run private-sellers-spa:server:development",
    "build:pre-prod": "ng build --configuration=preproduction",
    "build:pre-prod-ssr": "ng build --configuration=preproduction && ng run private-sellers-spa:server:preproduction",
    "build:prod-test": "ng build --configuration=prodtest",
    "build:prod-test-ssr": "ng build --configuration=prodtest && ng run private-sellers-spa:server:prodtest",
    "build:prod": "ng build --configuration=production",
    "build:prod-ssr": "ng build --configuration=production && ng run private-sellers-spa:server:production",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "lint": "eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "format": "prettier \"src/**/*.{html,css,scss}\" --write",
    "lint-and-format": "prettier-eslint \"src/**/*.{js,ts,html,css,scss}\" --write",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "^13.3.5",
    "@angular/cdk": "^13.3.4",
    "@angular/common": "^13.3.5",
    "@angular/compiler": "^13.3.5",
    "@angular/core": "^13.3.5",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "^13.3.5",
    "@angular/material": "^13.3.4",
    "@angular/material-moment-adapter": "^13.3.4",
    "@angular/platform-browser": "^13.3.5",
    "@angular/platform-browser-dynamic": "^13.3.5",
    "@angular/platform-server": "^13.3.5",
    "@angular/router": "^13.3.5",
    "@ng-select/ng-select": "^6.1.0",
    "@ngrx/store": "^13.1.0",
    "@nguniversal/common": "^13.1.0",
    "@nguniversal/express-engine": "^13.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@segment/consent-manager": "^5.6.0",
    "@sentry/node": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "@stripe/stripe-js": "^1.21.1",
    "algoliasearch": "^3.35.1",
    "angular-instantsearch": "^3.0.0-beta.5",
    "angularx-social-login": "^3.5.7",
    "bootstrap": "^4.6.0",
    "express": "^4.15.2",
    "instantsearch.css": "^7.4.5",
    "instantsearch.js": "^3.7.0",
    "ng-recaptcha": "^8.0.1",
    "ngx-auto-unsubscribe": "^3.0.1",
    "ngx-bootstrap": "^8.0.0",
    "ngx-cookie": "^5.0.2",
    "ngx-cookie-service": "^11.0.2",
